{
  "flags": {
    "feature-enabled": {
      "state": "ENABLED",
      "variants": {
        "on": true,
        "off": false
      },
      "defaultVariant": "off",
      "targeting": {
        "if": [
          {
            "in": [
              {
                "var": "env"
              },
              [
                "development",
                "test"
              ]
            ]
          },
          "on",
          "off"
        ]
      }
    }
  }
}